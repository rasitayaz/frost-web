<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android1="http://schemas.android.com/apk/res/android"
    >
    <item android1:id="@android:id/background">
        <shape android1:shape="rectangle">
            <solid android1:color="@color/colorPrimaryLighter"/>
        </shape>
    </item>
    <item android1:id="@android:id/progress">
        <clip android1:clipOrientation="horizontal" android1:gravity="left">
            <shape android1:shape="rectangle">
                <solid android1:color="@color/colorAccent"/>
            </shape>
        </clip>
    </item>
</layer-list>